{
  "name": "admit",
  "version": "0.1.0",
  "description": "admit/ban/check for array values like a proper bouncer",
  "main": "admit.js",
  "scripts": {
    "lint": "jshint admit.js && jshint test.js",
    "test": "npm run lint && node test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanve/admit.git"
  },
  "keywords": [
    "util",
    "utility",
    "in-array",
    "stack",
    "arrays",
    "array",
    "has"
  ],
  "author": "Ryan Van Etten",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryanve/admit/issues"
  },
  "homepage": "https://github.com/ryanve/admit#readme",
  "devDependencies": {
    "jshint": "^2.9.3"
  },
  "jshintConfig": {
    "asi": true,
    "boss": true,
    "browser": true,
    "debug": true,
    "devel": true,
    "eqnull": true,
    "evil": true,
    "expr": true,
    "globals": {},
    "latedef": "nofunc",
    "laxcomma": true,
    "maxerr": 5,
    "node": true,
    "sub": true,
    "supernew": true,
    "undef": true,
    "unused": "vars"
  }
